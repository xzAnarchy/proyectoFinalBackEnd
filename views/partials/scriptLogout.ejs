<script>

    /* Agrego evento en boton logout  */
    let botonLogout = document.getElementById('btnLogout')
        botonLogout.addEventListener('click', e => {
            /* funcion para desloguear nombre */
            fetch('http://localhost:8080/logout')
            .then(res => console.log('fetch ok'))
            .then(response => response.json())
                .then(json => console.log(json))
                .catch(err => console.log(err))
        })

</script>


